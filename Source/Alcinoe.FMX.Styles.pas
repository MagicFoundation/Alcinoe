      //Margins.Rect := ALScaleRect(Margins.DefaultValue, ARatio).RoundTo(-2);
      Padding.Rect := ALScaleRect(Padding.DefaultValue, ARatio).RoundTo(-2);
      TouchTargetExpansion := ALScaleRect(TRectF.Empty, ARatio).RoundTo(-2);

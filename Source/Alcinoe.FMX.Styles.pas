{****************************************************************************}
procedure ALResetShapeStyle(const AShape: TALShape; const ARatio: Single = 1);
begin
  With AShape do begin
    BeginUpdate;
    Try
      ALResetControlStyle(AShape, ARatio);
      Fill.Reset;
      Stroke.Reset;
      Stroke.Thickness := RoundTo(Stroke.DefaultThickness * ARatio, -2);
      Shadow.Reset;
    Finally
      EndUpdate;
    End;
  end;
end;


    DefaultCloseButton.Fill.ResourceName := ALSnackbarCloseButtonResourceName;
    DefaultCloseButton.Fill.ImageMargins.Rect := TRectF.Create(12*ARatio,12*ARatio,12*ARatio,12*ARatio).RoundTo(-2);

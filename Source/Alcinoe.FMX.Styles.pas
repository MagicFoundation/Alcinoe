    DefaultCloseButton.Margins.Rect := TRectF.Create(8*ARatio{Left}, 0{Top}, 0{Right}, 0{Bottom}).RoundTo(-2);
    DefaultCloseButton.Align := TALAlignLayout.MostRightCenter;

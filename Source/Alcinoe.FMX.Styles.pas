      StateStyles.Disabled.Fill.ResourceName := LPrevResourceName;
      //--Hovered--
      StateStyles.Hovered.StateLayer.ColorKey := 'Material3.Color.OnPrimary'; // md.sys.color.on-primary / md.ref.palette.primary100 / #FFFFFF

    Try
      ALResetRectangleStyle(AScrollBox);
      if HScrollBar <> nil then ALApplyMaterial3ScrollBarStyle(HScrollBar);
      if VScrollBar <> nil then ALApplyMaterial3ScrollBarStyle(VScrollBar);
    finally
      EndUpdate;
    end;
  end;
end;


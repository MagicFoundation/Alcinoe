      '  Result := TRectF.Create('+ADefaultValue+');'#13#10+
      'end;'#13#10+
      #13#10,
      Result,
      P1);
    //--
    P1 := 1;
    var LLst := TalStringListA.create;
    Try
      LLst.LineBreak := '.';
      LLst.Text := AClassName;
      if LLst.Count <= 0 then raise Exception.Create('Error 9DBDDEC9-6089-4832-BECE-705820A62559');

procedure TCanvasGradientMaterial.DoApply(const Context: TContext3D);
var
  I: Integer;
begin
  if FGradient.Style = TGradientStyle.Linear then
  begin
    case FGradient.Points.Count of
      2: Context.SetShaders(FVertexShader, FLinear2PS);
      3: Context.SetShaders(FVertexShader, FLinear3PS);
    else
      Context.SetShaders(FVertexShader, FLinear4PS);
    end;
    Context.SetShaderVariable('StartPoint', [Vector3D(FGradient.StartPosition.X, FGradient.StartPosition.Y, 0, 0)]);
    Context.SetShaderVariable('StopPoint', [Vector3D(FGradient.StopPosition.X, FGradient.StopPosition.Y, 0, 0)]);
  end else begin
    case FGradient.Points.Count of
      2: Context.SetShaders(FVertexShader, FRadial2PS);
      3: Context.SetShaders(FVertexShader, FRadial3PS);
    else
      Context.SetShaders(FVertexShader, FRadial4PS);
    end;
    Context.SetShaderVariable('RotationCenter', [Vector3D(FGradient.RadialTransform.RotationCenter.X, FGradient.RadialTransform.RotationCenter.Y, 0, 0)]);
  end;

  for I := 0 to Min(3, FGradient.Points.Count - 1) do
  begin
    Context.SetShaderVariable('Points[' + IntToStr(I) + '].Pos', [Vector3D(FGradient.Points[I].Offset, 0, 0, 0)]);
    Context.SetShaderVariable('Points[' + IntToStr(I) + '].Color', PremultiplyAlpha(FGradient.Points[I].Color));
  end;
end;

procedure TCanvasGradientMaterial.SetGradient(const Value: TGradient);
begin
  FGradient.Assign(Value);
end;

end.
